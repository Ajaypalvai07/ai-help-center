{
    "name": "ai-help-center",
    "description": "AI-powered Help Center with FastAPI backend",
    "runtime": "python3.9",
    "env": [
        {
            "key": "MONGODB_URL",
            "description": "MongoDB Atlas connection string"
        },
        {
            "key": "MONGODB_DB_NAME",
            "description": "MongoDB database name"
        },
        {
            "key": "SECRET_KEY",
            "description": "JWT secret key"
        },
        {
            "key": "CORS_ORIGINS",
            "description": "Allowed CORS origins"
        }
    ]
} 